                <div class="x_content">
                  <div id="account-form-container">
                    <%= form_for(:user , :html => {:class => users_path, :role => "form"}, url: users_path) do |f| %>
                    <div class ="form-horizontal well">
                      <h2>Edit Account</h2>
                      <%= devise_error_messages! %>
                      <hr/>
                        <div class="form-group">
                          <%= f.label :name, :class => "control-label col-sm-3" %>
                          <div class="col-sm-9">
                            <%= f.text_field :name, :autofocus => true, :class => 'form-control' %>
                          </div>
                        </div>
                        <div class="form-group">
                          <%= f.label :email, :class => "control-label col-sm-3" %>
                          <div class="col-sm-9">
                            <%= f.email_field :email, :class => 'form-control' %>
                          </div>
                        </div>
                        <div class="form-group">
                          <%= f.label :role, :class => "control-label col-sm-3" %>
                          <div class="col-sm-9">
                             <select name="cars" class = 'form-control'>
                              <option value="">----Please Select----</option>
                              <option value="admin">Admin</option>
                              <option value="manager">Manager</option>
                              <option value="emp">Employee</option>
                            </select>
                          </div>
                        </div>
                        <div class="form-group">
                          <%= f.label :password, :class => "control-label col-sm-3" %>
                          <div class="col-sm-9">
                            <%= f.password_field :password, :class => 'form-control' %>
                          </div>
                        </div>
                        <hr/>
                        <h6 id="sub2"></h6>
                        <div class="form-group">
                          <%= f.label :password_confirmation, :class => "control-label col-sm-3" %>
                          <div class="col-sm-9">
                            <%= f.password_field :password_confirmation, :class => 'form-control' %>
                          </div>
                        </div>
                        <div class="form-group">
                          <%= f.label :status, :class => "control-label col-sm-3" %>
                          <div class="col-sm-9">
                            <input type="radio", class="status" name="status_filter", value="Enabled", checked=status=="enable"><spam class= enb>Enable</spam>
                            <input type="radio", class="status" name="status_filter", value="Disbaled", checked=status=="disbale"> <spam class= enb>Disable</spam>
                      <hr/>
                      <div class="edit_butn">
                      <div class="form-buttons">
                      <%= f.submit 'Update', :class => "btn btn-primary" %></div>
                    <% end %>
                      <div class="form-buttons">
                       <%= button_to "Delete", registration_path(resource_name), :data => { :confirm => "Are you sure?" }, :method => :delete, :class => "btn btn-danger" %>
                      </div>
                      </div>
